<div>
    {{#each reviewList}}
    <div class="card p-3 px-4 mb-3">
        <div class="card mb-3">
            <div class="card-header">
                {{#with title}}
                <h5>{{userName}}</h5>
                <div class="pb-2">{{caption}}</div>
                <div>{{date}}</div>
                {{/with}}
            </div>
            <div class="card-body">
                {{#with reviewBody}}

                {{#with seller}}
                <div>{{title}}: {{value}}</div>
                {{/with}}

                {{#with rating}}
                <div class="py-3">{{title}}: {{value}}</div>
                {{/with}}

                {{#each text}}
                <p>{{this}}</p>
                {{/each}}

                {{#with advantages}}
                <div><b>{{title}}:</b></div>
                <div class="pb-3">{{text}}</div>
                {{/with}}

                {{#with disadvantages}}
                <div><b>{{title}}:</b></div>
                <div class="pb-3">{{text}}</div>
                {{/with}}

                <div class="row">
                    {{#each images}}
                    <div class="col-2 px-1">
                        <img class="card-img-top" src="{{this}}">
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
        <div class="row">
            {{#each coments}}
            <div class="co-10">
                <div class="card mb-3 ms-5 p-2">
                    <div><b>{{userName}}</b></div>
                    <div class="py-2">{{date}}</div>
                    <div>{{text}}</div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
        {{!-- {{#with title}}
        {{userName}}
        {{date}}
        {{text}}
        {{/with}} --}}
    </div>
    {{/each}}
</div>