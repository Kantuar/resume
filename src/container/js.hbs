<main>
    <div>
        <h2>{{name}}</h2>
    </div>
    <hr>
    <div>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Description</a></li>
                <li class="breadcrumb-item"><a href="#">Types</a></li>
                <li class="breadcrumb-item"><a href="#">Syntax</a></li>
                <li class="breadcrumb-item"><a href="#">Features</a></li>
                <li class="breadcrumb-item"><a href="#">Libraries</a></li>
                <li class="breadcrumb-item"><a href="#">Tools</a></li>
                <li class="breadcrumb-item active" aria-current="page">Community</li>
            </ol>
        </nav>
    </div>
    <div>
        <p><b>Description:</b>
            {{description}}</p>
        <p><b>History:</b>
            {{#with history}}
            {{year}}, {{founder}}
            {{/with}}
        </p>
    </div>
    <div>
        <button type="button" class="btn btn-dark">Types</button>
        <ul>
            {{#each types}}
            <li class="py-1">
                {{this}}
            </li>
            {{/each}}
        </ul>
    </div>
    <div>
        <button type="button" class="btn btn-dark">Syntax</button>
        <ul>
            {{#with syntax}}
            <li class="py-1"> <b><em>variables:</em></b>
                {{#each variables}}
                {{this}},
                {{/each}}
            </li>
            <li class="py-1"> <b><em>functions:</em></b>
                {{#each functions}}
                {{this}}
                {{/each}}
            </li>
            <li class="py-1"><b><em>conditionals:</em></b>
                {{#each conditionals}}
                {{this}},
                {{/each}}
            </li>
            <li class="py-1"><b><em>loops:</em></b>
                {{#each loops}}
                {{this}},
                {{/each}}
            </li>
            <li class="py-1"><b><em>classes:</em></b>
                {{#each classes}}
                {{this}}
                {{/each}}
            </li>
            {{/with}}
        </ul>
    </div>
    <div> <button type="button" class="btn btn-dark">Features</button>
        <ul>
            {{#each features}}
            <li class="py-1">{{this}}</li>
            {{/each}}
        </ul>
    </div>
    <div> <button type="button" class="btn btn-dark">Libraries</button>
        <ul>
            {{#each libraries}}
            <li class="py-1">{{this}}</li>
            {{/each}}
        </ul>
    </div>
    <div> <button type="button" class="btn btn-dark">Tools</button>
        <ul>
            {{#each tools}}
            <li class="py-1">{{this}}</li>
            {{/each}}
        </ul>
    </div>
    <div> <button type="button" class="btn btn-dark">Community</button>
        <ul>
            {{#each community}}
            <li class="py-1">{{name}}, {{type}}</li>
            {{/each}}
        </ul>
    </div>
</main>