<div>
    {{#with main}}
    <main>
        <div class="row">
            <div class="col-6">
                {{> title data="My educationss" big=false}}
                <ul class="ps-0">
                    {{#each educations}}
                    <li>
                        {{name}}
                        {{#unless isEnd}} (ще в процесі) {{/unless}}
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="col-6">
                {{> title data="My certificates" big=false}}
                <ul class="ps-0">
                    {{#each certificates}}
                    <li>
                        {{name}} ({{id}})
                    </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </main>
    {{/with}}
</div>